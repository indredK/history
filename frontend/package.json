    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "^7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@types/d3": "^7.4.3",
    "ahooks": "^3.9.6",
    "animate.css": "^4.1.1",
    "axios": "^1.13.2",
    "d3": "^7.9.0",
    "deck.gl": "^9.2.5",
    "echarts": "^6.0.0",
    "maplibre-gl": "^5.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-map-gl": "^8.1.0",
