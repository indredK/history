{
  "name": "history-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite",
    "start": "bun scripts/start.mjs",
    "build": "bun run type-check && bun --bun vite build",
    "build:analyze": "bun --bun vite build --mode analyze",
    "preview": "bun --bun vite preview",
    "deploy": "bun scripts/deploy.mjs",
    "lint": "bunx eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 100",
    "lint:fix": "bunx eslint src --ext ts,tsx --fix",
    "type-check": "bunx tsc --noEmit"
  },
  "dependencies": {
